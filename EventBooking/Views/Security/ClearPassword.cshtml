@model EventBooking.Controllers.ViewModels.ForgottenPasswordViewModel

@{
    using (Html.BeginForm("SetPassword", "Security",FormMethod.Post))
    {
    @Html.LabelFor(m => m.Password)
    @Html.Password("Password", Model.Password)
   
    @Html.HiddenFor(m => m.Token)
    <input type="submit" value="Skicka" />
    }
}