@model EventBooking.Controllers.ViewModels.SignedForActivity
@{
    ViewBag.Title = "WhoHasSignup";
}


    <table class="table table-hover">

        <tr>
            <th>Namn</th>
            <th>Från</th>
            <th>Till</th>
            <th>Matriel</th>
        </tr>


        @foreach (var user in Model.Session.SelectMany(s => s.Volunteers))
        {
            <tr>
                <td>@user.Name</td>

                @foreach (var session in user.Sessions)
                {
                    if (session.Activity.Id == Model.Id)
                    {
            
                    <td>
                        @session.FromTime
                    </td>
                    <td>
                        @session.ToTime
                    </td>
                    <td>Saker</td>
                    }
                }
            </tr>
        }

    </table>



